{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "POST",
                "GET",
                "PUT",
                "DELETE",
                "OPTIONS",
                "PATCH"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://auth-front-vercel.vercel.app"
            }
        }
    ]
}